{
  "name": "@verza/sdk",
  "version": "1.0.27",
  "description": "Verza Client & Server SDK",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./react": {
      "types": "./dist/framework-react.d.ts",
      "import": "./dist/framework-react.esm.js",
      "require": "./dist/framework-react.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "react": [
        "./dist/framework-react.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.mjs && tsc-alias -p tsconfig.json",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "ts-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "pre-commit": "npm run lint:fix && npm run ts-check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verzaio/verza-sdk.git"
  },
  "keywords": [
    "react",
    "three",
    "verza",
    "threejs"
  ],
  "author": "zo0r (https://github.com/zo0r)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/verzaio/verza-sdk/issues"
  },
  "homepage": "https://github.com/verzaio/verza-sdk#readme",
  "peerDependencies": {
    "react": ">=18.0",
    "react-dom": ">=18.0"
  },
  "dependencies": {
    "events": "^3.3.0",
    "socket.io-client": "^4.5.4",
    "socket.io-msgpack-parser": "^3.0.2",
    "three": "^0.149.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/events": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/react": ">=18.0",
    "@types/react-dom": ">=18.0",
    "@types/three": "^0.148",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "rollup": "^3.15.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  }
}
