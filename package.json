{
  "name": "@verza/sdk",
  "version": "2.0.51-dev.27",
  "description": "Verza Client & Server SDK",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run ts-check && rm -rf ./dist && rollup -c ./rollup.config.mjs && tsc-alias -p tsconfig.json",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src/ --ext .ts,.tsx --fix",
    "ts-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "pre-commit": "npm run lint:fix && npm run ts-check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.esm.js",
      "require": "./dist/utils.cjs.js"
    },
    "./react": {
      "types": "./dist/framework-react.d.ts",
      "import": "./dist/framework-react.esm.js"
    },
    "./react/client": {
      "types": "./dist/framework-react-client.d.ts",
      "import": "./dist/framework-react-client.esm.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.esm.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ],
      "react": [
        "./dist/framework-react.d.ts"
      ],
      "react/client": [
        "./dist/framework-react-client.d.ts"
      ],
      "config": [
        "./dist/config.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verzaio/verza-sdk.git"
  },
  "keywords": [
    "react",
    "three",
    "verza",
    "threejs",
    "games",
    "3d"
  ],
  "author": "zo0r (https://github.com/zo0r)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/verzaio/verza-sdk/issues"
  },
  "homepage": "https://github.com/verzaio/verza-sdk#readme",
  "dependencies": {
    "deep-equal": "^2.2.2",
    "events": "^3.3.0",
    "glob": "^10.3.10",
    "socket.io-client": "^4.7.2",
    "socket.io-msgpack-parser": "^3.0.2",
    "suspend-react": "^0.1.3",
    "three": "^0.156.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.9",
    "@vitejs/plugin-react": "^4.1.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/deep-equal": "^1.0.1",
    "@types/events": "^3.0.0",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.155.1",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rollup": "^3.28.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  }
}
